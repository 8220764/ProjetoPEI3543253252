<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="RecursosHumanos">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Ano" type="xs:integer"/>
                
                <xs:element name="Regiao">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="Distrito" type="xs:string"/>
                            <xs:element name="Concelho" type="xs:string"/>
                            
                            <xs:element name="Freguesia" type="xs:string" minOccurs="0"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="CapacidadeOperacional" type="xs:integer"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>