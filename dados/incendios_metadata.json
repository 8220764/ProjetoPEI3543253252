            "description": "Data completa do início do incêndio."
        },
        {
            "name": "hora",
            "type": "Integer",
            "description": "Hora de início (simplificada)."
        },
        {
            "name": "id_localizacao",
            "type": "Integer",
            "description": "id para o catálogo de localizações."
        },
        {
            "name": "id_causa",
            "type": "Integer",
            "description": "id para o catálogo de causas."
        },
        {
