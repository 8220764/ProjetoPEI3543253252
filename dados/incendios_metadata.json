{
    "dataset": "Histórico de Incêndios",
    "description": "Registo de ocorrências de incêndios florestais.",
    "fields": [
        {
            "name": "id_incendio",
            "type": "Integer",
            "description": "Identificador único do incidente."
        },
        {
            "name": "data",
