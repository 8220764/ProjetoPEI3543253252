        {
            "name": "areaTotal_ha",
            "type": "Decimal",
            "description": "Área total ardida em hectares."
        },
        {
            "name": "areaPov_ha",
            "type": "Decimal",
            "description": "Área de povoamento florestal ardida em hectares."
        },
        {
            "name": "areaMato_ha",
            "type": "Decimal",
            "description": "Área de mato ardida em hectares."
        },
        {
            "name": "areaAgric_ha",
            "type": "Decimal",
            "description": "Área agrícola ardida em hectares."
